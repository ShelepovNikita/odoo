<?xml version="1.0"?>
<odoo>
    <record id="information_system_view_form" model="ir.ui.view">
        <field name="name">information.system.product.tree</field>
        <field name="model">information.system.product</field>
        <field name="arch" type="xml">
            <form string="Test">
                <sheet>
                    <group>
                        <field name="name" string="Name"/>
                        <field name="description" string="Description"/>
                        <!-- <field name="warehouse_id" string="warehouse"/> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="information_system_view_tree" model="ir.ui.view">
        <field name="name">information.system.product.tree</field>
        <field name="model">information.system.product</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <field name="name"/>
                <field name="description"/>
            </tree>
        </field>
    </record>
    <record id="acts_view_tree" model="ir.ui.view">
        <field name="name">information.system.act.tree</field>
        <field name="model">information.system.act</field>
        <field name="arch" type="xml">
            <tree string="Tests">
                <header>
                    <button name="action_accept_act" type="object" string="Accept act"/>
                </header>
                <field name="name"/>
                <field name="date"/>
                <field name="status"/>
                <field name="count_products"/>
                <field name="expenses_income"/>
                <field name="user_id"/>
            </tree>
        </field>
    </record>
</odoo>